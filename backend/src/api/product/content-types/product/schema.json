{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "color": {
      "type": "string",
      "required": true
    },
    "sizes": {
      "type": "json",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "slug": {
      "type": "string"
    },
    "colorway": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "parent_product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::parent-product.parent-product",
      "inversedBy": "products"
    }
  }
}
